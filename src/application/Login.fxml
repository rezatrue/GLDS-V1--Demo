<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="121.0" prefWidth="279.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <Label layoutX="30.0" layoutY="20.0" prefHeight="20.0" prefWidth="65.0" text="Username : " />
      <TextField fx:id="usernameTF" layoutX="104.0" layoutY="18.0" promptText="username" />
      <Label layoutX="29.0" layoutY="56.0" prefHeight="20.0" prefWidth="65.0" text="Password : " />
      <TextField fx:id="passwordTF" layoutX="103.0" layoutY="54.0" promptText="password" />
      <Button fx:id="loginBtn" layoutX="133.0" layoutY="90.0" mnemonicParsing="false" onAction="#loginAction" prefHeight="25.0" prefWidth="83.0" text="Login" />
   </children>
</AnchorPane>
